{
  "name": "@saaskit/schema",
  "version": "0.1.0",
  "description": "Schema AST types for parsing Mermaid diagrams into a unified AST",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./validate": {
      "import": "./dist/validate.js",
      "types": "./dist/validate.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "saas",
    "schema",
    "ast",
    "mermaid",
    "parser",
    "code-generation",
    "api",
    "sdk"
  ],
  "author": "SaaSKit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/saaskit/saaskit.git",
    "directory": "packages/schema"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "mongo.do": "file:../../../mongo",
    "yaml": "2.8.2",
    "zod": "^3.23.0"
  }
}
